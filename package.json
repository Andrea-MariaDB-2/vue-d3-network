{
  "name": "@svenc56/vue-d3-network",
  "version": "0.2.5",
  "description": "Vue component to graph networks using d3-force",
  "main": "dist/vue-d3-network.umd.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "example": "vue-cli-service build --dest example",
    "dev": "vue-cli-service serve",
    "build": "yarn build-component && yarn clean:dist-example",
    "build-component": "vue-cli-service build --target lib --name vue-d3-network src/vue-d3-network.vue",
    "lint": "vue-cli-service lint",
    "clean:dist": "rm -r dist",
    "clean:example": "rm -r example",
    "clean:dist-example": "rm dist/demo.html",
    "publish:repo": "yarn build && yarn publish"
  },
  "author": "emi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SvenC56/vue-d3-network.git"
  },
  "bugs": {
    "url": "https://github.com/SvenC56/vue-d3-network/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "d3",
    "network",
    "d3-force",
    "graph",
    "component"
  ],
  "dependencies": {
    "d3-drag": "^1.2.5",
    "d3-force": "^1.2.1",
    "d3-selection": "^1.4.2",
    "d3-zoom": "^1.8.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^7.0.1",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^4.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "homepage": "https://github.com/SvenC56/vue-d3-network#readme"
}
